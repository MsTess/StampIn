<h1>My bookmarked Countries And Embassies</h1>
<div class="container py-5">
  <div class="row">
    <div class="col-6">
      <div class="bg-white rounded p-3 shadow">
        <div class="form">
          <%= simple_form_for(:search, url: visa_results_countries_path) do |f| %>
            <%= f.input :origin_country, collection: @countries %>
            <%= f.input :destination_country, collection: @countries%>
            <%= f.submit class: 'red_button'%>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
